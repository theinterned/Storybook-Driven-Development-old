{
  "presets": ["es2015", "react", "react-app"],
  "env": {
    "development": {
      "plugins": [
        [
          "transform-object-rest-spread",
          "react-transform", {
            "transforms": [{
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }]
          }
        ]
      ]
    }
  }
}
